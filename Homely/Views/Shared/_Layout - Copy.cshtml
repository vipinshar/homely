	<script type="text/javascript">
		$(function () {
			var offset = $("#social").offset();
			var topPadding = 200;
			$(window).scroll(function () {
				if ($(window).scrollTop() > offset.top) {
					$("#social").stop().animate({
						marginTop: $(window).scrollTop() - offset.top + topPadding
					});
				} else {
					$("#social").stop().animate({
						marginTop: 60
					});
				};
			});
		});
	</script>
